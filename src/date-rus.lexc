второе% :2. MONTH ;
третье% :3. MONTH ;
четвёртое% :4. MONTH ;
пятое% :5. MONTH ;
шестое% :6. MONTH ;
седьмое% :7. MONTH ;
восьмое% :8. MONTH ;
девятое% :9. MONTH ;
десятое% :1%0. MONTH ;
одиннадцатое% :11. MONTH ;
двенадцатое% :12. MONTH ;
тринадцатое% :13. MONTH ;
четырнадцатое% :14. MONTH ;
пятнадцатое% :15. MONTH ;
шестнадцатое% :16. MONTH ;
семнадцатое% :17. MONTH ;
восемнадцатое% :18. MONTH ;
девятнадцатое% :19. MONTH ;
двадцатое% :2%0. MONTH ;
двадцать% первое% :21. MONTH ;
двадцать% второе% :22. MONTH ;
двадцать% третье% :23. MONTH ;
двадцать% четвёртое% :24. MONTH ;
двадцать% пятое% :25. MONTH ;
двадцать% шестое% :26. MONTH ;
двадцать% седьмое% :27. MONTH ;
двадцать% восьмое% :28. MONTH ;
двадцать% девятое% :29. MONTH ;
